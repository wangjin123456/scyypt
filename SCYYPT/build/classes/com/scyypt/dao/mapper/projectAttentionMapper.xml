<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.scyypt.dao.ProjectAttentionDao">

	<insert id="addProjectAttention" parameterType="com.scyypt.entity.ProjectAttention"
		useGeneratedKeys="true" keyProperty="attentionId">
		INSERT INTO
		project_attention(accountUuid,itemId, name)
		VALUES(#{accountUuid}, #{itemId}, #{name})
	</insert>


</mapper>