}
*/
.datatable > table tr th:first-child {
    width: 10%;
}
.datatable > table tr th:nth-child(2) {
    width: 20%;
}
.datatable > table tr th:nth-child(3) {
    width: 50%;
}
.datatable > table tr th:nth-child(4) {
    width: 20%;
} 

