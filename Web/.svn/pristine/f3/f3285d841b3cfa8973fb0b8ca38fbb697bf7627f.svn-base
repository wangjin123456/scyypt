<div class="bg container-fluid">
  <form class="form-horizontal" role="form">
      <div class="form-group">
        <div class="col-sm-2">
          <!-- <input type="text" class="form-control" id="cooperationMethod" placeholder="请输入名字"> -->
          <select name="filter" id="cooperationMethod" class="form-control" (change)="filterSearch(filter)" [(ngModel)] = 'filter'>
            <option value="{{item.progressId}}" *ngFor = 'let item of filterData'>{{item.progressName}}</option>
          </select>
        </div>
        <div class="col-sm-4">
          <input [(ngModel)] = 'fuzzy' name="fuzzyquery" type="text" class="form-control" id="projectName" placeholder="按关键字搜索">
        </div>
        <div class="col-sm-3">
          <button class="btn btn-default" (click)='searchCode(fuzzy)'>搜索</button>
        </div>
      </div>
  </form>
  <div class="">
    <table class="table table-striped table-hover table-responsive">
      <thead>
        <tr>
          <th></th>
          <th>项目名称</th>
          <th>合作方式</th>
          <th>建筑面积</th>
          <th>发起人</th>
          <th>悬赏金额</th>
          <th>发起时间</th>
          <th>最后跟进时间</th>
          <th>进度</th>
          <th>操作</th>
          <th></th>
        </tr>
      </thead>
      <tbody *ngFor = 'let project of projectListdata; let i = index'>
        <tr>
          <td>
              <a (click) = 'upFollow( project.accountUuid, project.itemId, $event, i, project.itemProgress)' class="check-revision btn btn-xs btn-default collapsed">+</a>
          </td>
          <td>{{project.itemName}}</td>
          <td>{{project.itemCooperation}}</td>
          <td>{{project.itemaArea}}m<sup>2</sup></td>
          <td>{{project.userName}}</td>
          <td>悬赏金额</td>
          <td>{{project.itemCreateDatetime | slice:0:10}}</td>
          <td>{{project.itemFollowDatetime | slice:0:10}}</td>
          <!-- 这里是展示项目的进度对应的字段 1对应首次拜访 2对应洽谈跟进 3对应业绩核算 4对应完成 progressId-->
          <td *ngIf="project.itemProgress==1">{{this.informationVisit.firstVisit}}</td>
          <td *ngIf="project.itemProgress==2">{{this.informationVisit.alignVisit}}</td>
          <td *ngIf="project.itemProgress==3">{{this.informationVisit.threeVisit}}</td>
          <td *ngIf="project.itemProgress==4">{{this.informationVisit.fulfilVisit}}</td>

          <td>操作</td>
          <td>
            <div class="toolColumn">
              <a href="javascript:;" class="" (click) = 'clickToolMenu(i)'>更多</a>
              <!-- <ng-template #toolmenu + i></ng-template> -->
              <ul class="tool hide" [class.show] = 'i == toolIndex'>
                <li><span class="icon-tool icon-give-up"></span><a href="javascript:;">放弃</a></li>
                <li><span class="icon-tool reimbursement"></span><a href="javascript:;">报销审批</a></li>
                <li><span class="icon-tool contract"></span><a href="javascript:;">合同审批</a></li>
                <li><span class="icon-tool mail"></span><a href="javascript:;">邮件</a></li>
                <li><span class="icon-tool signature"></span><a href="javascript:;">签章</a></li>
                <li><span class="icon-tool file-cabinets"></span><a href="javascript:;">文件柜</a></li>
                <li><span class="icon-tool case"></span><a href="javascript:;">Case</a></li>
              </ul>
            </div>
          </td> 
        </tr>
        <tr class="revision-item warning panel-collapse collapse" [class.showFollowUp] = 'i == toolIndexFollowUp' >
          <td colspan="11">
            <div class="followUp">
              <div class="showColumn">
                <span><b>项目进度：</b></span>
                <span [class.fontWeight]="project.itemProgress==1" (click)='getDataList(project.itemId,1)'><a href="javascript:;">{{this.informationVisit.firstVisit}} &gt;</a></span> 
                <span [class.fontWeight]="project.itemProgress==2" (click)='getDataList(project.itemId,2)'><a href="javascript:;">{{this.informationVisit.alignVisit}} &gt;</a></span>
                <span [class.fontWeight]="project.itemProgress==3" (click)='getDataList(project.itemId,3)'><a href="javascript:;">{{this.informationVisit.threeVisit}} &gt;</a></span>
                <span [class.fontWeight]="project.itemProgress==4" (click)='getDataList(project.itemId,4)'><a href="javascript:;">{{this.informationVisit.fulfilVisit}}</a></span>
              </div>
              <div class="bodyColumn">
                <!-- 他人消息 -->
                <!-- <div class="leftd" *ngFor="let mess of messageList">
                  <span ng-class="leftd_h">
                      <img ng-src="./img/c_pic.pn" />
                  </span>
                  <div class="speech left">
                      {{mess}}
                  </div>
                </div> -->

                <div class="clearfix" *ngFor="let newsData of newsDataList">
                    <p class="infoTimeline" *ngIf="newsData.itemId==pubcliItemId">{{newsData.date | date:'yyyy-MM-dd HH:mm:ss'}}</p>
                    <!-- 他人的消息 -->
                    <span ng-class="leftd_h" *ngIf="newsData.accountUuid!=pubcliAccountUuid&&newsData.itemId==pubcliItemId">
                      <img [src]="newsData.iconPath" width="60px" />
                    </span>
                    <div class="line-block" *ngIf="newsData.accountUuid!=pubcliAccountUuid&&newsData.itemId==pubcliItemId">
                      <div class="line-block">
                        <p class="p-margin-8">{{newsData.name}}</p>
                        <div class="speech left" *ngIf='newsData.type==1&&newsData.itemId==pubcliItemId'>
                          <p class="pMargin-0">{{newsData.message}}</p>
                        </div>
                        <div class="speech left" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==5" >
                            {{newsData.fileName}}
                            <img width="40px" src="{{filePDF}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech left" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==6" >
                            {{newsData.fileName}}
                            <img width="40px" src="{{fileXlsx}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech left" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==1" >
                            <img width="40px" src="{{newsData.fileName}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech left" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==2" >
                              {{newsData.fileName}}
                            <img width="40px" src="{{fileDocx}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech left" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==3" >
                              {{newsData.fileName}}
                            <img width="40px" src="{{fileTxt}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech left" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==4" >
                              {{newsData.fileName}}
                            <img width="40px" src="{{fileWar}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>
                      </div>
                    </div>
                    <!-- 自己的消息 -->
                    <div class="pull-right text-right" *ngIf="newsData.accountUuid==pubcliAccountUuid&&newsData.itemId==pubcliItemId">
                      <div class="line-block" *ngIf="newsData.accountUuid==pubcliAccountUuid&&newsData.itemId==pubcliItemId">
                        <div class="line-block">
                          <p class="p-margin-0">{{newsData.name}}</p>
                          <div class="speech right" *ngIf='newsData.type==1&&newsData.itemId==pubcliItemId'>
                            <p class="pMargin-0">{{newsData.message}}</p>
                          </div>

                          <div class="speech right" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==5" >
                            {{newsData.fileName}}
                            <img width="40px" src="{{filePDF}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech right" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==6" >
                            {{newsData.fileName}}
                            <img width="40px" src="{{fileXlsx}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech right" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==1" >
                            <img width="40px" src="{{newsData.fileName}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech right" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==2" >
                              {{newsData.fileName}}
                            <img width="40px" src="{{fileDocx}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech right" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==3" >
                              {{newsData.fileName}}
                            <img width="40px" src="{{fileTxt}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>

                          <div class="speech right" *ngIf="newsData.type==2&&newsData.itemId==pubcliItemId&&newsData.fileType==4" >
                              {{newsData.fileName}}
                            <img width="40px" src="{{fileWar}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                          </div>
                        </div>
                      </div>
                      <span ng-class="rightd_h">
                        <img [src]="newsData.iconPath" width="60px" />
                      </span>
                    </div>
                </div>
                <!-- 已发消息 -->
                <!-- <div class="clearfix padding_any" *ngFor="let mlSend of messageListSelf">
                    <p class="infoTimeline">{{mlSend.time | date:'yyyy-MM-dd HH:mm:ss'}}</p>
                    <div class="pull-right text-right">
                      <div class="line-block">
                        <p class="p-margin-0">{{mlSend.name}}</p>
                        <div *ngIf="mlSend.type==1" class="speech right">
                          {{mlSend.message}}
                        </div>

                        <div *ngIf="mlSend.fileType==1" class="speech right">
                            <img width="40px" src="{{mlSend.message}}" onerror="this.src='../../assets/image/touxiang.jpg'" title="发送返回的文件">
                        </div>

                        <div *ngIf="mlSend.fileType==5" class="speech right">
                            {{mlSend.fileName}}
                            <img width="40px" src="{{filePDF}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                        </div>

                        <div *ngIf="mlSend.fileType==6" class="speech right">
                          {{mlSend.fileName}}
                          <img width="40px" src="{{fileXlsx}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                        </div>

                        <div *ngIf="mlSend.fileType==2" class="speech right">
                            {{mlSend.fileName}}
                            <img width="40px" src="{{fileDocx}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                        </div>

                        <div *ngIf="mlSend.fileType==3" class="speech right">
                            {{mlSend.fileName}}
                            <img width="40px" src="{{fileTxt}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                        </div>

                        <div *ngIf="mlSend.fileType==4" class="speech right">
                            {{mlSend.fileName}}
                            <img width="40px" src="{{fileWar}}" onerror="this.src='../../assets/image/touxiang.jpg'">
                        </div>
                       
                      </div>
                      <span ng-class="rightd_h">
                        <img [src]="mlSend.iconPath" width="60px" />
                      </span>
                    </div>
                </div> -->

                <!-- <div class="clearfix" *ngFor="let mlSendImg of messageImgListSelf">
                    <p class="infoTimeline">{{mlSendImg.time | date:'yyyy-MM-dd HH:mm:ss'}}</p>
                    <div *ngIf="mlSendImg.type!=2" class="pull-right sendFileBlock">
                      <a style="display: inline-block;width:100%; height:88px;" class="speech right" href="{{mlSendImg.message}}" download="{{mlSendImg.fileName}}">
                        {{mlSendImg.fileName}}
                      </a>
                    </div>
                    <div *ngIf="mlSendImg.type==2" class="speech right">
                        <a style="display: inline-block;width:100%;" class="speech right" href="{{mlSendImg.message}}" download="{{mlSendImg.fileName}}">
                          <img width="40px" src="{{mlSendImg.message}}" alt="发送文件" title="发送返回的文件">
                        </a>
                    </div>
                </div> -->
              </div>
              <div class="sendColumn">
                <div class="sendColumnTop">
                      <span title="文件">
                        <a href="javascript:" class="sendFile">
                          <input class="inFileBlock" type="file" (change)="getFilePath(project.itemId,1,$event,project.itemProgress)">
                        </a> 
                      </span>
                      <span title="费用保险">
                        <a>
                          <img src="../../assets/image/icon/reimbursement.png" alt="费用">
                        </a>
                      </span>
                      <span title="方案制作"><a><img src="../../assets/image/icon/scheme.png" alt="方案"></a></span>
                      <span title="接待申请"><a><img src="../../assets/image/icon/Receptiontoapplyfor.png" alt="申请"></a></span>
                      <span title="任务悬赏"><a><img src="../../assets/image/icon/reward.png" alt="悬赏"></a></span>
                      <span title="时间提醒">
                        <a>
                          <img (click)="reminderTime()" src="../../assets/image/icon/time.png" alt="时钟" class="sendTimepaddingRight">
                        </a>
                      </span>
                      <div class="pull-right">
                        <!-- <button class="btn btn-default" [class.disabled]='project.itemProgress==1'>上一阶段</button> -->
                        <button class="btn btn-default" *ngIf="project.itemProgress==4" [class.disabled]='project.itemProgress==4' data-toggle="modal" data-target="#myModal" >已完成</button>
                        <button class="btn btn-default" *ngIf="project.itemProgress==1" [class.disabled]='project.itemProgress==4' data-toggle="modal" data-target="#myModal" (click)='getProjectDate(project.accountUuid,project.itemId,project.itemProgress)'>{{this.informationVisit.alignVisit}}</button>
                        <button class="btn btn-default" *ngIf="project.itemProgress==2" [class.disabled]='project.itemProgress==4' data-toggle="modal" data-target="#myModal" (click)='getProjectDate(project.accountUuid,project.itemId,project.itemProgress)'>{{this.informationVisit.threeVisit}}</button>
                        <button class="btn btn-default" *ngIf="project.itemProgress==3" [class.disabled]='project.itemProgress==4' data-toggle="modal" data-target="#myModal" (click)='getProjectDate(project.accountUuid,project.itemId,project.itemProgress)'>{{this.informationVisit.fulfilVisit}}</button>
                      </div>
                      <!-- <span title="时间提醒" class="pull-right">
                        <a>
                          <img (click)="reminderTime()" src="../../assets/image/icon/time.png" alt="时钟" class="sendTimepaddingRight">
                        </a>
                        {{remindTime | date:'yyyy-MM-dd hh:mm:ss'}}
                      </span> -->
                </div>
                <div class="sendColumnBody">
                  <div class="sendCount" contenteditable="true" #sendObj></div>
                  <div class="sendbtn">
                    <button class="btn" (click) = 'messageSend(project.itemId,1,$event,project.itemProgress)'>发送</button>
                  </div>
                </div>
              </div>
            </div>
          </td>
        </tr>
      </tbody>
      <!-- 弹框 -->
      <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title" id="myModalLabel">温馨提示</h4>
            </div>
            <div class="modal-body">
              是否切换到下一阶段
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
              <button type="button" class="btn btn-primary" data-dismiss="modal" (click)='doNextStatus()'>确认</button>
            </div>
          </div>
        </div>
      </div>

    </table>
    <!-- 时间提醒 -->
    <div class="reminderBlock" [hidden]="submitted">
      <p class="reminder-title">时间提醒设置</p>
      <form class="form-horizontal" #reminderTimeForm = 'ngForm' (ngSubmit)="onSubmit()">
        <div class="form-group">
          <label for="email" class="col-sm-3 control-label">邮箱</label>
          <div class="col-sm-8">
            <input *ngIf="blockRecipients == false" class="form-control" type="text" (input)="OAsearchFor(oaListInput)" #oaListInput>
            <!-- 这里绑定收件人数据 -->
            <select *ngIf="OAdataList == true" class="form-control" (change)='recipientsChecked(oaList)' [(ngModel)]="reminderTimeEmail" required name="oaList" #oaList="ngModel">
                <option *ngFor="let pow of listRecipientsData" [value] = "pow | objstr">{{pow.uname}}</option>
            </select>
            <!-- <div >
              <ul>
                <li *ngFor="let pow of listRecipientsData">{{pow.uname}}</li>
              </ul>
            </div> -->

          </div>
        </div>
        <div class="form-group sendTimeR">
            <label for="reminderEndtime" class="col-sm-3 control-label">提醒时间</label>
            <div class="col-sm-8">
                <input class="form-control reminderIcon" name="time" type="hidden" readonly [value]="reminderTimeDate | date:'yyyy-MM-dd hh:mm:ss'" >
                <p-calendar [(ngModel)]="reminderTimeDate " [name]="time" #time="ngModel" [showTime] = "true" [hourFormat] = '24' [utc] = "true" ></p-calendar>
            </div>
        </div>
        <div class="form-group sendTimeMarginBottom">
            <label for="reminderBody" class="col-sm-3 control-label">提醒信息内容</label>
            <div class="col-sm-8">
              <textarea name="body" id="reminderBody" cols="30" rows="3" class="form-control" [(ngModel)]="reminderTimeEmailInfo"></textarea>
            </div>  
        </div>
        <div class="text-center btnpadding">
            <button type="reset" class="btn btnSize" (click)="reminderTimeHide(); reminderTimeForm.reset()">取 消</button>
            <button type="submit" class="btn btnSize btnPrimary" [disabled]="!reminderTimeForm.form.valid">确定</button>
        </div>
      </form>
    </div>

  </div>
  <div class="row myprojrct-pagination">
    <app-pagination [pageParams]="pageContent" (changeCurPage)="getPageData($event)"></app-pagination>
  </div>
</div>